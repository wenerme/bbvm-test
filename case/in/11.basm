; 11 | 字符串转换为浮点数
; 返回: 浮点数
; 参数: r3:字符串

ld int r3, STR_F

in r1, 11

out 0, r0;>0
out 0, r2;>0
out 1, r3;>123.019997
out 5, r1;|>123.019997


ld int r3, STR_FAIL
in r1, 11
out 0, r2;>0
out 1, r3;>ABCD
out 5, r1;|>000.000

exit
; 选择了一个能够正确转换为浮点数的数值
data STR_F char "123.019997",0
data STR_FAIL char "ABCD",0